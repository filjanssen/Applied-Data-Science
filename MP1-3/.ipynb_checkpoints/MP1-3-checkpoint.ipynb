   "source": [
    "def fib(value):\n",
    "    sq = []\n",
    "    \n",
    "    for i in range(value+1):\n",
    "        current = i\n",
    "        sq.append(current**2)\n",
    "    \n",
    "    return sq\n",
    "\n",
    "print(\"The square of first 30 numbers are: \" + str(fib(30)))\n",
